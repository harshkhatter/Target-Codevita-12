#!/bin/python3

import math
import os
import random
import re
import sys

# Complete the miniMaxSum function below.
def miniMaxSum(arr):
    #check the max and min of array
    #calculate the sum and print
    print(sum(arr)-max(arr) ,  sum(arr)-min(arr))
    
    
if __name__ == '__main__':
    arr = list(map(int, input().rstrip().split()))

    miniMaxSum(arr)

